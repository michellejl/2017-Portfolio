    a(href="https://github.com/IonicaBizau/github-calendar" target="_blank") IonicaBizau
  // Include github-calendar library
  script(src="https://cdn.rawgit.com/IonicaBizau/github-calendar/gh-pages/dist/github-calendar.min.js")
